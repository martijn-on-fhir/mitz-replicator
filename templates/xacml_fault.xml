<?xml version="1.0" encoding="UTF-8"?>
<soap:Envelope xmlns:soap="http://www.w3.org/2003/05/soap-envelope">
  <soap:Body>
    <soap:Fault>
      <soap:Code>
        <soap:Value>{{ .FaultCode }}</soap:Value>
        <soap:Subcode>
          <soap:Value>{{ .FaultSubcode }}</soap:Value>
        </soap:Subcode>
      </soap:Code>
      <soap:Reason>
        <soap:Text>{{ .FaultReason }}</soap:Text>
      </soap:Reason>
      <soap:Detail>{{ .FaultDetail }}</soap:Detail>
    </soap:Fault>
  </soap:Body>
</soap:Envelope>
